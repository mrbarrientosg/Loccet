<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="20.0">
         <children>
            <Button fx:id="nuevoMaterialBT" mnemonicParsing="false" onAction="#nuevoMaterial" prefHeight="40.0" text="Nuevo Material">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="40.0" text="Modificar Material">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
            <Button fx:id="retirarBT" mnemonicParsing="false" onAction="#retirarMaterial" prefHeight="40.0" text="Retirar">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
            <Button fx:id="agregarBT" mnemonicParsing="false" onAction="#agregarMaterial" prefHeight="40.0" text="Agregar">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
            <Button fx:id="eliminarBT" mnemonicParsing="false" onAction="#eliminar" prefHeight="40.0" text="Eliminar">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
            <Separator visible="false" HBox.hgrow="ALWAYS" />
            <Button fx:id="salirBT" mnemonicParsing="false" prefHeight="40.0" text="Salir">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <TableView fx:id="tablaInventario" prefHeight="600.0" prefWidth="792.0">
        <columns>
            <TableColumn fx:id="fechaIngresoCL" prefWidth="85.0" text="Fecha ingreso" />
          <TableColumn fx:id="idMaterialCL" prefWidth="57.0" text="ID" />
            <TableColumn fx:id="nombreMaterialCL" prefWidth="65.0" text="Nombre" />
          <TableColumn fx:id="descripcionCL" prefWidth="307.0" text="Descripcion" />
            <TableColumn fx:id="cantidadCL" minWidth="0.0" text="Cantidad" />
            <TableColumn fx:id="udsCL" prefWidth="43.0" text="UDS" />
            <TableColumn fx:id="retiroCL" prefWidth="75.0" text="Retiro" />
            <TableColumn fx:id="fechaRetiroCL" prefWidth="75.0" text="Fecha retiro" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
