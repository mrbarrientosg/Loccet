               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
            <Label layoutX="216.0" layoutY="98.0" prefHeight="17.0" prefWidth="169.0" text="DESCRIPCION DEL MATERIAL" translateX="-100.0">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
            <TextArea fx:id="descripcionTF" layoutX="166.0" layoutY="125.0" prefHeight="67.0" prefWidth="269.0" translateX="-100.0" wrapText="true" />
            <HBox layoutX="5.0" layoutY="235.0" spacing="5.0">
               <children>
                  <TextField fx:id="cantidadTF">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
                  <ComboBox fx:id="unidadCB" prefHeight="25.0" prefWidth="81.0" />
