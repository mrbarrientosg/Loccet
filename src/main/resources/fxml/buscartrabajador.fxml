<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane styleClass="shadow-root" stylesheets="@../css/searchworker.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="8.0" stylesheets="@../css/searchworker.css">
         <children>
            <Label styleClass="title1" text="Buscar Trabajador">
               <VBox.margin>
                  <Insets left="16.0" right="16.0" top="8.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="searchField" promptText="Buscar trabajador">
               <styleClass>
                  <String fx:value="textfield" />
                  <String fx:value="textfield-gray" />
               </styleClass>
               <VBox.margin>
                  <Insets left="16.0" right="16.0" />
               </VBox.margin>
            </TextField>
            <ListView fx:id="listView" editable="true" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets left="16.0" right="16.0" />
               </VBox.margin>
               <styleClass>
                  <String fx:value="listViewGrey" />
                  <String fx:value="paragraph-light" />
               </styleClass>
            </ListView>
            <ButtonBar>
              <buttons>
                  <StackPane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0">
                     <children>
                        <AnchorPane styleClass="button-shadow-white">
                           <StackPane.margin>
                              <Insets left="5.0" right="5.0" top="10.0" />
                           </StackPane.margin>
                           <padding>
                              <Insets bottom="5.0" />
                           </padding>
                        </AnchorPane>
                      <Button fx:id="doneButton" mnemonicParsing="false" prefHeight="38.0" prefWidth="78.0" styleClass="button-fill-blue" text="Aceptar" />
                     </children>
                  </StackPane>
                  <Button fx:id="cancelButton" mnemonicParsing="false" prefWidth="78.0" styleClass="button-label-gray" text="Cancelar" />
              </buttons>
               <VBox.margin>
                  <Insets bottom="10.0" left="16.0" right="16.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
         <styleClass>
            <String fx:value="bg-white" />
            <String fx:value="corner-pane" />
         </styleClass>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
