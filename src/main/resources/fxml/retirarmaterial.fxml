<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" styleClass="backgroundWhite" stylesheets="@../css/retirematerial.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox spacing="20.0">
         <children>
            <Label styleClass="title2" text="Retirar Material">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <TextField fx:id="retirarTF" promptText="Cantidad a retirar">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <styleClass>
                  <String fx:value="textField" />
                  <String fx:value="textFieldGrey" />
               </styleClass>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
         </VBox.margin>
      </VBox>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button fx:id="retirarBT" defaultButton="true" mnemonicParsing="false" onAction="#cantidadItem" styleClass="buttonBlue" text="Retirar" />
            <Button fx:id="cancelarBT" mnemonicParsing="false" onAction="#cancelar" styleClass="buttonFillBlue" text="Cancelar" />
        </buttons>
         <VBox.margin>
            <Insets bottom="16.0" left="16.0" right="16.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
</VBox>
