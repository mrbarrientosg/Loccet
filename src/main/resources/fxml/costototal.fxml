<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane styleClass="shadow-root" stylesheets="@../App.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="250.0" prefWidth="400.0">
         <children>
            <StackPane>
               <children>
                  <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" StackPane.alignment="CENTER">
                     <children>
                        <ImageView fitHeight="250.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <image>
                              <Image url="@../images/total.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
                  <VBox alignment="CENTER" spacing="30.0">
                     <children>
                        <HBox spacing="8.0">
                           <children>
                              <Label styleClass="title3" text="Monto contractual :" />
                              <Label fx:id="montoContractualLB" maxWidth="1.7976931348623157E308" styleClass="title3" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox spacing="8.0">
                           <children>
                              <Label styleClass="title3" text="Gasto :" />
                              <Label fx:id="gastoLB" maxWidth="1.7976931348623157E308" styleClass="title3" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox spacing="8.0">
                           <children>
                              <Label styleClass="title3" text="Monto actual :" />
                              <Label fx:id="montoActualLB" maxWidth="1.7976931348623157E308" styleClass="title3" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                     </children>
                     <StackPane.margin>
                        <Insets bottom="16.0" left="16.0" right="16.0" />
                     </StackPane.margin>
                  </VBox>
               </children>
            </StackPane>
            <Button fx:id="salirBT" mnemonicParsing="false" onAction="#salir" prefWidth="90.0" styleClass="button-fill-red" text="Salir">
               <VBox.margin>
                  <Insets bottom="8.0" top="4.0" />
               </VBox.margin>
            </Button>
         </children>
         <styleClass>
            <String fx:value="bg-white" />
            <String fx:value="corner-pane" />
         </styleClass>
      </VBox>
   </children>
</StackPane>
